/* pages/index/index.wxss */
.header {
  position: fixed;
  display: flex;
  width: 100%;
  height: 50px;
  justify-content: space-between;
  align-items: center;
  background: rgba(200,200,200,0.3);
  z-index: 99;
  box-sizing: border-box;
}
.btn,.btn::after{
  content:none;
  background: none;
  border:none;
}
navigator{
  width: 80%;
  height: 30px;
  border: 1px solid #ddd;
  background: #f5f5f5;
  border-radius: 30px;
}
swiper image{
  width: 100%;
  height: 480rpx;
}
swiper{
  height: 480rpx;
}

.item {
  width: 355rpx;
  margin: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 1px solid #ddd;
  border-radius: 5px;
  overflow: hidden;
  box-shadow: 5px 5px 5px #ddd;
  box-sizing: border-box;
}
.item image{
  width: 100%;
}